<form>
  <fieldset>
    <field
      name="file"
      type="file"
      label="COM_TOWNOFFICAL_IMPORTFORM_FILE_LABEL"
      description="COM_TOWNOFFICAL_IMPORTFORM_FILE_DESC"
      size="80"
      default=""
      required="true"
      class="inputbox"
    />
    <field
      name="useCSVheader"
      type="checkbox"
      label="COM_TOWNOFFICAL_IMPORTFORM_USECSVHEADER_LABEL"
      description="COM_TOWNOFFICAL_IMPORTFORM_USECSVHEADER_DESC"
      class="inputbox"
      value="1"
    />
    <field
      name="delimiter"
      type="list"
      label="COM_TOWNOFFICAL_IMPORTFORM_DELIMITER_LABEL"
      description="COM_TOWNOFFICAL_IMPORTFORM_DELIMITER_DESC"
      class="inputbox"
      default=","
      >
      <option value=",">COM_TOWNOFFICAL_COMMA</option>
      <option value=";">COM_TOWNOFFICAL_SEMICOLON</option>
      <option value="\t">COM_TOWNOFFICAL_TAB</option>
    </field>
    <field
      name="quote"
      type="list"
      label="COM_TOWNOFFICAL_IMPORTFORM_QUOTE_LABEL"
      description="COM_TOWNOFFICAL_IMPORTFORM_QUOTE_DESC"
      class="inputbox"
      default='"'
      >
      <option value='"'>COM_TOWNOFFICAL_DQUOTE</option>
      <option value="'">COM_TOWNOFFICAL_SQUOTE</option>
    </field>
  </fieldset>
</form>

